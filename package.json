{
  "name": "@medianaura/yalkem",
  "version": "0.2.1",
  "repository": "https://github.com/MedianAura/yalkem",
  "license": "MIT",
  "author": "MedianAura",
  "type": "module",
  "main": "dist/entry.js",
  "types": "dist/entry.d.ts",
  "bin": {
    "yalkem": "./bin/run.js"
  },
  "files": [
    "./bin",
    "./dist"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -p tsconfig.build.json",
    "check": "concatenate check",
    "check:knip": "knip",
    "check:stryker": "stryker run",
    "fix": "concatenate fix",
    "release": "automaton version",
    "start": "cross-env NODE_ENV=development node ./bin/run.js",
    "test:coverage": "open-cli coverage/lcov-report/index.html",
    "test:unit": "vitest --coverage.enabled --coverage.all --coverage.reporter=lcov",
    "test:unit:coverage": "vitest --reporter basic --reporter junit --outputFile.junit=junit.xml --coverage.enabled --coverage.reporter=text-summary --coverage.reporter=cobertura --coverage.reporter=lcov --watch=false",
    "watch": "shx rm -rf dist && tsc -p tsconfig.build.json --watch"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^12.1.0",
    "@inquirer/prompts": "^5.0.5",
    "@spongex/os-appdata-path": "^0.2.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^12.0.0",
    "dotenv-flow": "^4.1.0",
    "log-update": "^6.0.0",
    "pino": "^9.1.0",
    "pino-pretty": "^11.2.0",
    "read-pkg": "^9.0.1",
    "reflect-metadata": "^0.2.2",
    "sprintf-js": "^1.1.3",
    "ts-custom-error": "^3.3.1",
    "tsyringe": "^4.8.0",
    "update-notifier": "^7.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@medianaura/automaton": "^0.1.2",
    "@medianaura/concatenate": "^0.2.1",
    "@medianaura/komity": "^0.1.1",
    "@rushstack/eslint-patch": "^1.10.3",
    "@stryker-mutator/vitest-runner": "^8.2.6",
    "@types/node": "^20.14.2",
    "@types/sprintf-js": "^1.1.4",
    "@types/update-notifier": "^6.0.8",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitest/coverage-v8": "^1.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-n": "^17.8.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-plugin-zod": "^1.4.0",
    "happy-dom": "^14.12.0",
    "knip": "^5.18.2",
    "open-cli": "^8.0.0",
    "prettier": "^3.3.2",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0",
    "unplugin-auto-import": "^0.17.6",
    "vitest": "^1.6.0"
